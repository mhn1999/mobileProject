<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.notifications.SpeedmeterFragment" >
    <!-- list_item_cell_info.xml -->
    <com.farimarwat.supergaugeview.SuperGaugeView
        android:id="@+id/speedMeterSim1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:duration="500"
        app:gaugeText="Upload Speed"
        app:gaugeTextColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/speedMeterSim2"
        app:progressBackground="gauge_keyhole_14" />

    <com.farimarwat.supergaugeview.SuperGaugeView
        android:id="@+id/speedMeterSim2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:duration="500"
        app:gaugeText="Upload Speed"
        app:gaugeTextColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/speedMeterSim1"
        app:progressBackground="gauge_keyhole_14" />


</androidx.constraintlayout.widget.ConstraintLayout>